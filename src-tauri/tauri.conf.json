{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "Gym Manager",
	"version": "1.0.3",
	"identifier": "com.gym-manager.dc",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:1420",
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build"
	},
	"app": {
		"windows": [
			{
				"title": "Gym Manager",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false,
				"maximized": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
 "createUpdaterArtifacts": true,
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"updater": {
			"active": true,
			"endpoints": ["https://api.github.com/repos/c-danil0o/gym_manager/releases/latest"],
			"dialog": true,
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJDM0RBNEQyOEIzRENDQkUKUldTK3pEMkwwcVE5TExXT1BnUEpTSHJVNHdvRnduOVpjTlV6alBBZHNac0Y0UnF3RllCRzJZcjIK",
			"windows": {
				"installMode": "basicUi",
				"installerArgs": []
			}
		}
	}
}
